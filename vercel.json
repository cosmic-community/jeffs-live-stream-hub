{
  "buildCommand": "bun run build",
  "outputDirectory": ".next",
  "installCommand": "bun install",
  "framework": "nextjs",
  "regions": ["iad1"],
  "functions": {
    "app/**/*.{js,ts,tsx}": {
      "runtime": "nodejs18.x"
    }
  },
  "env": {
    "COSMIC_BUCKET_SLUG": "@cosmic_bucket_slug",
    "COSMIC_READ_KEY": "@cosmic_read_key",
    "COSMIC_WRITE_KEY": "@cosmic_write_key"
  }
}